<div class="container mt-4">
    <h3 class="mb-4">Admin List</h3>
    <table class="table table-bordered table-striped">
      <thead class="table-dark">
        <tr>
          <th>#</th>
          <th>Email</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Image</th>
          <th colspan="2">action</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((item, index) => { %>
            <tr>
              <td><%= index + 1 %></td>
              <td><%= item.firstname %></td>
              <td><%= item.lastname %></td>
              <td><%= item.email %></td>
              <td>
                <img src="<%= item.image %>" alt="User Image" width="60" height="60">
              </td>
              <td>
                <button><a href="/deleteData?id=<%= item._id %>">Delete</a></button>
                <button><a href="/editData?id=<%= item._id %>">Edit</a></button>
              </td>
            </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
  